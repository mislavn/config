config timeserver 'ntp_2'
        list server '0.openwrt.pool.ntp.org'
        list server '7.openwrt.pool.ntp.org' #comment
        list server test
        list server '5.openwrt.pool.ntp.org'
list server '2.openwrt.pool.ntp.org'
        option enabled '1'
        option enable_server 0




